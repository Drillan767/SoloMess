<p id="notice"><%= notice %></p>

<h1>Basics</h1>

<table>
  <thead>
    <tr>
      <th>Base title</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Github</th>
      <th>Linkedin</th>
      <th>Viadeo</th>
      <th>Resume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @basics.each do |basic| %>
      <tr>
        <td><%= basic.base_title %></td>
        <td><%= basic.facebook %></td>
        <td><%= basic.twitter %></td>
        <td><%= basic.github %></td>
        <td><%= basic.linkedin %></td>
        <td><%= basic.viadeo %></td>
        <td><%= basic.resume %></td>
        <td><%= link_to 'Show', basic %></td>
        <td><%= link_to 'Edit', edit_basic_path(basic) %></td>
        <td><%= link_to 'Destroy', basic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Basic', new_basic_path %>
